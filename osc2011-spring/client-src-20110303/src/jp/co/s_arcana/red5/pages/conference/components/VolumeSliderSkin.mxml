<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:d="http://ns.adobe.com/fxg/2008/dt">
	<fx:Metadata>[HostComponent("spark.components.HSlider")]</fx:Metadata>
	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>
	<fx:DesignLayer d:userLabel="volume_off">
		<s:Graphic id="slider_icon_on">
			<s:BitmapImage smooth="true" source="@Embed('assets/slider_icon_over.png')" d:userLabel="icon_over" x="0" y="0"/>
		</s:Graphic>
		<s:Graphic id="slider_icon_off" visible="false">
			<s:BitmapImage smooth="true" source="@Embed('assets/slider_icon_off.png')" d:userLabel="icon_off" x="0" y="0"/>
		</s:Graphic>
		
	</fx:DesignLayer>
	<fx:DesignLayer id="designlayer1" d:userLabel="volume_over">
		<s:Button id="track" skinClass="jp.co.s_arcana.red5.pages.conference.components.VolumeSliderTrackSkin" d:userLabel="トラック" x="19" y="5"/>
		<s:Button id="thumb" skinClass="jp.co.s_arcana.red5.pages.conference.components.VolumeSliderThumbSkin" d:userLabel="サム" x="19" y="1"/>
	</fx:DesignLayer>
</s:Skin>
